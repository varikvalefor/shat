        [_]
      )
    open ‚â°-Reasoning
    map‚ÇÇ-cong = DLP.map-cong ‚àò DLP.map-cong

  porkcos : {n : ‚Ñï}
          ‚Üí (a b : Fin n)
          ‚Üí (d : a ùîΩ.‚â§ b)
          ‚Üí (c : Fin n)
          ‚Üí (_‚â°_
              (pork $ (show a , show b) , show c)
