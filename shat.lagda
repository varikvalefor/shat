    open ‚â°-Reasoning

  rel : (s : String)
      ‚Üí (t : ‚Ñï)
      ‚Üí (c : Char)
      ‚Üí false ‚â° isDigit c
      ‚Üí (_‚â°_
          (show t ‚à∑ degjygirzu s)
          (degjygirzu $ show t ++ ùïä.fromChar c ++ s))
  rel s t c j = sym $ begin
    d (show t ++ ùïä.fromChar c ++ s) ‚â°‚ü® refl ‚ü©
    d' (tL $ show t ++ ùïä.fromChar c ++ s) ‚â°‚ü® refl ‚ü©
    _ ‚â°‚ü® tildist (show t) (ùïä.fromChar c ++ s) ‚ñπ cong d' ‚ü©
    d' (tL (show t) ++ tL (ùïä.fromChar c ++ s)) ‚â°‚ü® refl ‚ü©
    _ ‚â°‚ü® {!!} ‚ñπ cong (d' ‚àò _++_ (tL (show t))) ‚ü©
    d' (tL (show t) ++ c ‚à∑ tL s) ‚â°‚ü® {!!} ‚ü©
    d' (tL $ show t) ++ d' (tL s) ‚â°‚ü® refl ‚ü©
    d' (tL $ show t) ++ d s ‚â°‚ü® refl ‚ü©
    d (show t) ++ d s ‚â°‚ü® pav (sym ‚àò fL‚àòtL) t ‚ñπ cong (_++ d s) ‚ü©
